<div class="group">
  <h3> <%= f.object.object_id %> </h3>

  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :search_text %>
    <%= f.text_field :search_text %>
    <br>
    <%= f.label :geocode %>
    <%= f.text_field :geocode %>
    <br>
    <%= f.label :group %>
    <%= f.text_field :group %>
    <br>
    <%= f.label :priority %>
    <%= f.text_field :priority %>
    <br>


    <!--<form class="form-horizontal" role="form">-->
    <!-- Arrival -->
    <div class="form-group">
      <%= f.label :Arrive, class: 'col-sm-2 control-label' %>
      <div class="col-sm-5">
        <%= f.text_field :arrive_after, class: 'form-control time-start-A', placeholder: 'after' %>
      </div>
      <div class="col-sm-5">
        <%= f.text_field :arrive_before, class: 'form-control time-start-B', placeholder: 'before' %>
      </div>
    </div>
    <!-- Arrival Ends -->

    <!-- Duration -->
    <div class="form-group">
      <%= f.label :Duration, class: 'col-sm-2 control-label' %>
      <div class="col-sm-5">
        <%= f.text_field :min_duration, class: 'form-control dur-A', placeholder: 'min' %>
      </div>
      <div class="col-sm-5">
        <%= f.text_field :max_duration, class: 'form-control dur-B', placeholder: 'max' %>
      </div>
    </div>
    <!-- Duration Ends -->

    <!-- Departure -->
    <div class="form-group">
      <%= f.label :Depart, class: 'col-sm-2 control-label' %>
      <div class="col-sm-5">
        <%= f.text_field :depart_after, class: 'form-control time-end-A', placeholder: 'after' %>
      </div>
      <div class="col-sm-5">
        <%= f.text_field :depart_before, class: 'form-control time-end-B', placeholder: 'before' %>
      </div>
    </div>
    <!-- Departure Ends -->

    <!-- Priority -->
    <div class="form-group">
      <%= f.label :Priority, class: 'col-sm-2 control-label' %>
      <div class="col-sm-5">
        <select class="form-control priority">
          <option>1</option>
          <option>2</option>
          <option>3</option>
          <option>4</option>
          <option>5</option>
        </select>
      </div>
    </div>
    <!-- Priority Ends -->


    <!-- Button set -->
    <div class="form-group">
      <div class="col-sm-12">
        <div class="col-sm-6">
          <button type="reset" class="btn btn-warning col-sm-12">
            Reset
          </button>
        </div>
        <div class="col-sm-6">
          <button type="button" class="btn btn-danger remove-btn col-sm-12">
            Remove
          </button>
        </div>
      </div>
    </div>
    <!-- Button set ends -->
    <!--</form>-->
  </div>
</div>